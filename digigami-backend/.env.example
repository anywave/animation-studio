# Digigami Backend Configuration
# Copy to .env and modify as needed

# Server
DIGIGAMI_HOST=0.0.0.0
DIGIGAMI_PORT=8765
DIGIGAMI_DEBUG=true

# CORS (comma-separated origins)
DIGIGAMI_CORS_ORIGINS=["http://localhost:8080","http://127.0.0.1:8080","https://digigami.me"]

# Model weights directory
DIGIGAMI_WEIGHTS_DIR=weights

# Processing
DIGIGAMI_DEFAULT_STYLE=kingdom-hearts
DIGIGAMI_OUTPUT_SIZE=512
DIGIGAMI_MAX_IMAGE_SIZE=1280
DIGIGAMI_JPEG_QUALITY=90

# Device (auto-detected if not set)
# DIGIGAMI_DEVICE=cuda
